import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int x = 2;
        int y = 5;
        int sum = x + y;
        System.out.println(x != y);
        String firstName = "atanas";
        String secondName = "Atanas";
        System.out.println(firstName.equals(secondName.toLowerCase()));
        System.out.println(firstName == "atanas" && sum != 7);
        System.out.println(firstName == "atanas" || sum != 7);
    }
}
